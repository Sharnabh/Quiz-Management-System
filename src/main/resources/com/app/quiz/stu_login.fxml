<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="565.0" prefWidth="670.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.app.quiz.stuLoginController">
    <children>
        <AnchorPane prefHeight="84.0" prefWidth="670.0" style="-fx-background-color: orange;">
            <children>
                <Label layoutX="216.0" layoutY="15.0" prefHeight="54.0" prefWidth="237.0" text="Student Login" textFill="#fcf6f6">
                    <font>
                        <Font size="37.0" />
                    </font>
                </Label>
            </children>
        </AnchorPane>
        <Label layoutX="146.0" layoutY="122.0" prefHeight="29.0" prefWidth="110.0" text="Username" textFill="#151414">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <TextField fx:id="UnameField" layoutX="146.0" layoutY="152.0" prefHeight="26.0" prefWidth="381.0" promptText="Username" />
        <Label layoutX="146.0" layoutY="192.0" prefHeight="29.0" prefWidth="110.0" text="Password" textFill="#151414">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <Button fx:id="LoginBtn" layoutX="280.0" layoutY="311.0" mnemonicParsing="false" onAction="#loginBtnOnAction" prefHeight="26.0" prefWidth="110.0" text="Login" />
        <Button fx:id="RegToLogBtn" layoutX="238.0" layoutY="362.0" mnemonicParsing="false" onAction="#regToLogOnAction" prefHeight="26.0" prefWidth="197.0" text="Do not Have an Account?" />
      <Label fx:id="popup" alignment="CENTER" layoutX="169.0" layoutY="267.0" prefHeight="30.0" prefWidth="327.0" textFill="#ea0b0b">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <PasswordField fx:id="PassField" layoutX="145.0" layoutY="222.0" prefHeight="26.0" prefWidth="382.0" promptText="Password" />
      <Button fx:id="backBtn" layoutX="281.0" layoutY="413.0" mnemonicParsing="false" onAction="#backBtnOnAction" prefHeight="26.0" prefWidth="110.0" text="Back" />
    </children>
</AnchorPane>
